            "server_url": "https://api.elevenlabs.io"
        },
        "parameters": {
            "type": "object",
            "properties": {
                "path": {
                    "type": "object",
                    "description": "Path parameters",
                    "properties": {
                        "voice_id": {
                            "type": "string",
                            "description": "The ID of the voice whose settings are to be retrieved. Use the Get Voices endpoint to list all available voices."
