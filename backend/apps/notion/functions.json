                    "description": "Header parameters",
                    "properties": {
                        "Notion-Version": {
                            "type": "string",
                            "description": "The version of the Notion API to use. Defaults to 2022-06-28.",
                            "default": "2022-06-28"
                        }
                    },
                    "required": ["Notion-Version"],
                    "visible": [],
                    "additionalProperties": false
                },
                "body": {
                    "type": "object",
