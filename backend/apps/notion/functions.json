        },
        "parameters": {
            "type": "object",
            "properties": {
                "path": {
                    "type": "object",
                    "description": "Path parameters",
                    "properties": {
                        "page_id": {
                            "type": "string",
                            "description": "The ID of the page to archive or restore"
                        }
                    },
                    "required": ["page_id"],
                    "visible": ["page_id"],
                    "additionalProperties": false
