                                    "properties": {
                                        "active": { "type": "boolean" }
                                    },
                                    "required": [],
                                    "visible": ["active"],
                                    "additionalProperties": false
                                },
                                "captcha_solver": {
                                    "type": "object",
                                    "properties": {
                                        "active": { "type": "boolean" }
                                    },
                                    "required": [],
                                    "visible": ["active"],
                                    "additionalProperties": false
                                },
                                "headless": {
                                    "type": "object",
                                    "properties": {
                                        "active": { "type": "boolean" }
                                    },
                                    "required": [],
                                    "visible": ["active"],
                                    "additionalProperties": false
                                },
                                "viewport": {
                                    "type": "object",
                                    "properties": {
                                        "width": { "type": "integer" },
                                        "height": { "type": "integer" }
                                    },
                                    "required": [],
                                    "visible": ["width", "height"],
                                    "additionalProperties": false
                                }
                            },
                            "required": [],
                            "visible": [
                                "profile",
                                "adblock",
                                "popup_blocker",
                                "captcha_solver",
                                "headless",
                                "viewport"
