                "body": {
                    "type": "object",
                    "properties": {
                        "session": {
                            "type": "object",
                            "properties": {
                                "recording": {
                                    "type": "object",
                                    "properties": {
                                        "active": { "type": "boolean" }
                                    },
                                    "required": [],
                                    "visible": ["active"],
                                    "additionalProperties": false
                                },
                                "proxy": {
                                    "type": "object",
                                    "properties": {
                                        "type": {
                                            "type": "string",
