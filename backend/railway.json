{
  "$schema": "https://railway.com/railway.schema.json",
  "build": {
    "nixpacksPlan": {
      "phases": {
        "install": {
          "dependsOn": ["setup"],
          "cmds": ["python3 install pip && pip install uv && uv sync --all-extras --no-install-project"]
        }
      }
    }
  }
}
